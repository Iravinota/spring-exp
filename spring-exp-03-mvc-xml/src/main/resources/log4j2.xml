<?xml version="1.0" encoding="UTF-8"?>
<configuration status="info">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} {%t} %-5level %logger{36} - %m%n" />
        </Console>
    </Appenders>

    <loggers>
        <root level="info">
            <AppenderRef ref="stdout" />
        </root>
    </loggers>
</configuration>